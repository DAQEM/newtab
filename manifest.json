{
  "manifest_version": 3,
  "name": "Minimal New Tab",
  "version": "1.0",
  "description": "A minimal new tab page with shortcuts and custom wallpaper.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
    "permissions": [
    "storage",
    "unlimitedStorage",
    "geolocation"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
